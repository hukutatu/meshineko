<h1>画像投稿フォーム</h1>
  <%= form_with model: @recipes, url:public_recipes_path, method: :post do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>料理名</h4>
      <%= f.text_field :title %>
    <h4>説明</h4>
      <%= f.text_area :capion %>
    <%= f.submit '投稿' %>
  <% end %>